        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "editorMode": "code",
          "expr": "# Combine same and cross zone communications (divide by 2 due to egress/ingress metrics)\n(\n  label_replace(\n    sum by (source_az, target_az) (\n      increase(traffic_total{source_az!=\"\", target_az!=\"\"}[$__interval]) * on(source_az, target_az) group_left() \n      (\n        label_replace(\n          group by (source_az) (traffic_total), \n          \"target_az\", \"$1\", \"source_az\", \"(.*)\"\n        )\n      )\n    ),\n    \"zone_type\", \"same_zone\", \"\", \"\"\n  ) / 2\n  or\n  label_replace(\n    sum by (source_az, target_az) (increase(traffic_total{source_az!=\"\", target_az!=\"\"}[$__interval]))\n    unless on(source_az, target_az) \n    (\n      label_replace(\n        group by (source_az) (traffic_total), \n        \"target_az\", \"$1\", \"source_az\", \"(.*)\"\n      )\n    ),\n    \"zone_type\", \"cross_zone\", \"\", \"\"\n  )\n) / 2",
          "instant": false,
          "legendFormat": "{{source_az}} -> {{target_az}}",
          "range": true,
          "refId": "A"
        }
      ],
      "title": "Traffic across AZs",
      "type": "timeseries"
